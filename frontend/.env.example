# Environment Variables for SLA Digital Frontend
# Copy this file to .env.local and update with your actual values

# Backend API Configuration
NEXT_PUBLIC_API_URL=http://localhost:5000
NEXT_PUBLIC_WS_URL=ws://localhost:5000

# Authentication (if needed)
NEXT_PUBLIC_JWT_SECRET=your-jwt-secret-here

# App Configuration
NEXT_PUBLIC_APP_NAME=SLA Digital Dashboard
NEXT_PUBLIC_APP_VERSION=2.0.0
NEXT_PUBLIC_APP_ENV=development

# Analytics (optional)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=
NEXT_PUBLIC_MIXPANEL_TOKEN=

# Error Tracking (optional)
NEXT_PUBLIC_SENTRY_DSN=

# Feature Flags (optional)
NEXT_PUBLIC_ENABLE_WEBSOCKETS=true
NEXT_PUBLIC_ENABLE_EXPORTS=true
NEXT_PUBLIC_ENABLE_NOTIFICATIONS=true

# Production API URLs (for reference)
# NEXT_PUBLIC_API_URL=https://your-production-api.com
# NEXT_PUBLIC_WS_URL=wss://your-production-api.com